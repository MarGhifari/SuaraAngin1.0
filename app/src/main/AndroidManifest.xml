<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.komc.suaraanginrev">


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="SuaraAngin"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.SuaraAnginRev">
        <activity
            android:name=".activities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.NewsDetail"/>
        <activity android:name=".activities.SettingActivity"/>
        <activity android:name=".activities.FragmentActivity"/>
        <activity android:name=".fragment.BusinessActivity"/>
        <activity android:name=".fragment.EntertainActivity"/>
        <activity android:name=".fragment.HealthActivity"/>
        <activity android:name=".fragment.ScienceActivity"/>
        <activity android:name=".fragment.SportActivity"/>
        <activity android:name=".fragment.TechActivity"/>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>